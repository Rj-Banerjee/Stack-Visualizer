    "client": "cd frontend && npm run dev"
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "keywords": ["stack", "visualizer", "data-structures", "react", "express"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  }
}



