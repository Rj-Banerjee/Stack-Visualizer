{
  "homepage": "https://Rj-Banerjee.github.io/stack-visualizer"
  "name": "stack-visualizer",
  "version": "1.0.0",
  "description": "Full-stack Stack Visualizer - Parentheses Checker",
  "main": "index.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
